<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="ttk.Frame" id="main_frame" named="True">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="pack">
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Entry" id="entry" named="True">
        <property name="textvariable">int:val</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="set_scrap_button" named="True">
        <property name="command" type="command" cbtype="simple">e_set_scrap</property>
        <property name="text" translatable="yes">Set Scrap</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="set_hull_button" named="True">
        <property name="command" type="command" cbtype="simple">e_set_hull</property>
        <property name="text" translatable="yes">Set Hull</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="set_fuel_button" named="True">
        <property name="command" type="command" cbtype="simple">e_set_fuel</property>
        <property name="text" translatable="yes">Set Fuel</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="set_missiles_button" named="True">
        <property name="command" type="command" cbtype="simple">e_set_missiles</property>
        <property name="text" translatable="yes">Set Missiles</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="heal_crew_button" named="True">
        <property name="command" type="command" cbtype="simple">e_heal_crew</property>
        <property name="text" translatable="yes">Heal Crew</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="kill_enemy_button" named="True">
        <property name="command" type="command" cbtype="simple">e_kill_enemy</property>
        <property name="text" translatable="yes">Kill Enemy</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="invincibility_checkbox" named="True">
        <property name="command" type="command" cbtype="simple">e_invincibility</property>
        <property name="text" translatable="yes">Invincibility</property>
        <property name="variable">int:invincibility_var</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="superweapon_textbox" named="True">
        <property name="command" type="command" cbtype="simple">e_superweapons</property>
        <property name="text" translatable="yes">Super Weapons</property>
        <property name="variable">int:superweapons_var</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Checkbutton" id="infinite_drones_checkbox" named="True">
        <property name="command" type="command" cbtype="simple">e_infinite_drones</property>
        <property name="text" translatable="yes">Infinite Drones</property>
        <property name="variable">int:var_infinite_drones</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="padx">10</property>
          <property name="pady">10</property>
          <property name="row">6</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
